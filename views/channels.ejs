<h1 id="channel"><%= channel %></h1>

<ul id="messages">
  <% messages.forEach(message => { %> 
    <li><%= message.name %>: <%= message.chat_message %></li>
  <% }) %>
</ul>

<form id="form">
  <input id="input" autocomplete="off" name="<%= channel %>" />
  <button>Send</button>
</form>
<p id="name"><%= name %></p>

<script src="/socket.io/socket.io.js"></script>
<script src="/public/scripts/script.js"></script>
